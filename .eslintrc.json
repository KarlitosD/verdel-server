{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-tabs": "off",
        "indent": ["error", "tab"],
        "quotes":["error", "double"]
    }
}
